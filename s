#!/bin/bash
if [ $# = 0 ]
then
	kaba sisyphos.kaba
fi
if [ $# = 1 ]
then
	kaba sisyphos.kaba "$1"
fi
if [ $# = 2 ]
then
	kaba sisyphos.kaba "$1" "$2"
fi
if [ $# = 3 ]
then
	kaba sisyphos.kaba "$1" "$2" "$3"
fi
if [ $# = 4 ]
then
	kaba sisyphos.kaba "$1" "$2" "$3" "$4"
fi
if [ $# = 5 ]
then
	kaba sisyphos.kaba "$1" "$2" "$3" "$4" "$5"
fi
if [ $# = 6 ]
then
	kaba sisyphos.kaba "$1" "$2" "$3" "$4" "$5" "$6"
fi
if [ $# = 7 ]
then
	kaba sisyphos.kaba "$1" "$2" "$3" "$4" "$5" "$6" "$7"
fi
if [ $# = 8 ]
then
	kaba sisyphos.kaba "$1" "$2" "$3" "$4" "$5" "$6" "$7" "$8"
fi

