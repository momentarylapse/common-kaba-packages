use terminal
use parser
use runtime



void main()
	print("welcome to kabama")
	Runtime r
	string[] history
	while true
		try
			let s = fancy_read_command(">>> ", history)
			if s != ""
				let t = r.eval(s)
				print(str(t))
		#except SyntaxError as e
		#	print("Syntax error: {{e}}")
		except Exception as e
			if str(e) in ["end", "break"]
				print("exit")
				break
			else
				print("Error: {{e}}")
