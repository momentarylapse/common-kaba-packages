

struct Address
	var host: string
	var port: i32
#	func extern __init__()
#	func extern __delete__()

class Socket
	var uuid: i32
	var handle: i32
	
	func extern mut __init__()
	func extern mut __delete__()
	func extern mut accept() -> xfer[Socket]
	func extern mut close()
	func extern mut set_blocking(block: bool)
	func extern mut set_target(target: Address)
	func extern get_sender() -> Address
	func extern mut read(size: i32) -> bytes
	func extern mut write(buf: bytes) -> bool
	func extern can_read() -> bool
	func extern can_write() -> bool
	func extern is_connected() -> bool

func extern listen(port: i32, block: bool) -> xfer[Socket]
func extern connect(addr: string, port: i32) -> xfer[Socket]
func extern create_udp(port: i32) -> xfer[Socket]


